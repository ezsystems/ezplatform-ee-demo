<div class="container">
        <div class="col-md-12 main-content">
            <div class="row landing-page__zones">
                <div class="landing-page__zone landing-page__zone--{{ zones[0].id }} landing-page__zone--left col-9" data-ez-zone-id="{{ zones[0].id }}">
                    {% if zones[0].blocks %}
                        {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[0].blocks} %}
                    {% endif %}
                </div>
                <div class="landing-page__zone landing-page__zone--{{ zones[1].id }} landing-page__zone--right col-3" data-ez-zone-id="{{ zones[1].id }}">
                    {% if zones[1].blocks %}
                        {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[1].blocks} %}
                    {% endif %}
                </div>
            </div>
        </div>
</div>
